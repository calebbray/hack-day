<script>
  window.addEventListener("load", function () {
    const params = new URLSearchParams(location.search);
    const formattedParams = {};

    for (const [k, v] of params) {
      formattedParams[k] = v;
    }

    loadImage(formattedParams).then((data) => {
      const p = document.getElementById('occ-text');
      console.log('loading the image data', data)
      const loader = document.getElementById('loader');
      loader.remove();
      const image = document.getElementById('jeeves-says');
      if (image) {
        image.src = data;
      }
    });
  })
</script>
